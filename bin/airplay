#!/usr/bin/env php
<?php

set_time_limit(0);

require __DIR__.'/../vendor/autoload.php';

$app = new ArchFizz\PhpAirPlay\Application(new Symfony\Component\Filesystem\Filesystem());
$app->add(new ArchFizz\PhpAirPlay\MirrorCommand());
$app->add(new ArchFizz\PhpAirPlay\PlayVideoCommand());
$app->run();
